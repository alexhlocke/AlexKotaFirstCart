--main x limits for player
--[[
 these can change depending on
 special rooms / states
--]]
main_xl_limit=20
main_xr_limit=410
frame=0

player={
  x=63, y=70
 ,default_y=70
 ,speed=1
 ,sprt_idle=0
 ,sprt_walk=1
 --player x limits
 ,xl_limit=main_xl_limit
 ,xr_limit=main_xr_limit
 --player hitbox
 ,hbox={x1,y1,x2,y2}
 --states: walking,idle,fishing,frozen (for menus)
 ,state="walking"
 ,tail=3
}


--== player functions ==--------


--update player
function update_player()
 local s=player
 frame+=1
 
 --== movement ==--
 
 --if state=walking move
 if(s.state=="walking") then
 
		--sprint if 🅾️
		if(btn(🅾️))then
		 s.speed=2
		else
		 s.speed=1
		end
		
		--move player
		if(btn(⬅️))then
		 s.x-=s.speed
		end
		if(btn(➡️))then
		 s.x+=s.speed
		end
		
		--clamp pos
		if(s.x<s.xl_limit)then
		 s.x=s.xl_limit
		end
		if(s.x>s.xr_limit)then
		 s.x=s.xr_limit
		end 
 end
 
 --== interations ==--
 
 --update hbox
 local hb = s.hbox
 hb.x1=s.x-3
 hb.x2=s.x+18
 hb.y1=s.y-3
 hb.y2=s.y+18
 
 --press ❎ to interact
 if(btnp(❎)) then
  --interact with shit
 end
 
	--== idle animation ==-
		if(frame%4==0) then
		cat_tail()
		end
end


--draw player
function draw_player()
		local s=player
		cat_idle(s.x,s.y)
 
 --debug draw hbox
 fillp(▒)
 local hb=s.hbox
 rect(hb.x1,hb.y1,hb.x2,hb.y2,8)
 fillp()
 
	map(0,0,0,0,128,128)
end


--== camera ==-------------------

cam={x=0,y=0}

function update_cam()
 local px=player.x

 --stop camera for static rooms
 if(px>0 and px<128)then
  cam.x=0
 end
 if(px>384)then
  cam.x=384
 end
 
 --scrolling town
 if(px>128 and px<384)then
  cam.x=px-59
  if(cam.x<128)cam.x=128
  if(cam.x>256)cam.x=256
 end

 --move camera
 camera(cam.x,cam.y)
end

--cat idle tail animation
function cat_tail()
	if(player.tail < 5) then
		player.tail+=1
	else
		player.tail=3
	end
	return player.tail
end

function cat_idle(x,y)
		spr(0,x,y,2,3)
		spr(player.tail,x-7,y+8,1,2)
end
