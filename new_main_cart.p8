end


function draw_game()
 cls(2)
 
 --draw bg
 draw_background()
 
 --draw map
 map(0,0,0,0,128,128)

 --draw player
 draw_player()
 
 --==fishing==--
  draw_hook()
  --draw fish
 
 --draw foreground details

 		
 
 --draw main menu
 if(show_menu)draw_main_menu()
 
 print("this is the game",5,5,12)
end 


