--update
function update_mg_player()
 local s=mg_player
 local spd=s.speed
 if(btn(‚¨ÖÔ∏è))then
  s.x-=spd
  if(s.x<0)s.x=0
 end
 if(btn(‚û°Ô∏è))then
  s.x+=spd
  if(s.x>127)s.x=127
